{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "Components/*": ["./src/components/*"],
      "Pages/*": ["./src/pages/*"],
      "Services/*": ["./src/services/*"],
      "Store/*": ["./src/store/*"],
      "Utils/*": ["./src/utils/*"]
    }
  }
}
